<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>View Data</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }
        th, td {
            border: 1px solid #ccc;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f4f4f4;
        }
    </style>
</head>
<body>
    <h1>Data Entries</h1>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Last Name</th>
                <th>First Name</th>
                <th>Email</th>
                <th>Gender</th>
            </tr>
        </thead>
        <tbody id="data-table-body">
            <% data.forEach(entry => { %>
                <tr>
                    <td><%= entry.Id %></td>
                    <td><%= entry.lastName %></td>
                    <td><%= entry.firstName %></td>
                    <td><%= entry.email %></td>
                    <td><%= entry.gender %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
